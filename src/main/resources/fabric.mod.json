  "description": "A FabricMC library mod, that allows you to dynamically create, load, unload and delete worlds.",
  "authors": [
    "DrTheo_ (@dr.theo - discord)"
  ],
  "contact": {
    "repo": "https://github.com/DrTheodor/mc-multidim"
  },
  "license": "LGPL-3.0",
  "icon": "assets/multidim/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.drtheo.multidim.MultiDimMod"
    ]
  },
  "mixins": [
    "multidim.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
